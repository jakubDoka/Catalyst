
fn other(a: int, b: int, c: int) -> int {
    ret a + b * c
}

fn bti(b: bool) -> int {
    ret if b { 1 } else { 0 }
}

fn structure_test() -> int {
    pin something = Something::{
        a: 1
        b {
            x: 1
        }
        e: true
    }

    something.a - something.b.x 
}

struct Something {
    a: int
    b: SomethingElse
    e: bool
}

struct SomethingElse {
    x: int 
}